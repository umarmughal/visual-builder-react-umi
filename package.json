{
  "name": "visual-builder-react-umi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "deploy": "cd .cloudflare/ && wrangler publish",
    "build-demo": "REACT_APP_AUTHENTICATED=true umi build",
    "build-deploy": "yarn vb-full && yarn build-demo && yarn deploy && yarn vb-seed",
    "vb": "node ./scripts/vb.js",
    "vb-full": "REDIRECT_URL=/dashboard/alpha node ./scripts/generate.js --config=config-full.json",
    "vb-seed": "node ./scripts/generate.js --config=config-seed.json"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "antd": "^4.10.0",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.19.0",
    "bootstrap": "^4.5.3",
    "c3": "^0.4.24",
    "chalk": "^4.1.0",
    "chart.js": "^2.9.3",
    "chartist": "^0.11.4",
    "chartist-plugin-tooltips-updated": "^0.1.2",
    "classnames": "^2.2.6",
    "cli-select": "^1.1.2",
    "connected-react-router": "^6.8.0",
    "d3-dsv": "^1.2.0",
    "d3-format": "^1.4.4",
    "d3-time-format": "^2.2.3",
    "draft-js": "^0.11.5",
    "dva": "^2.6.0-beta.22",
    "firebase": "^8.1.1",
    "history": "^4.10.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "2.27.0",
    "nprogress": "^0.2.0",
    "prismjs": "^1.22.0",
    "qs": "^6.9.3",
    "react": "^17.0.1",
    "react-c3js": "^0.1.20",
    "react-chartist": "^0.14.3",
    "react-chartjs-2": "^2.9.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-draft-wysiwyg": "^1.14.5",
    "react-draggable": "^4.4.3",
    "react-highlight-words": "^0.16.0",
    "react-jvectormap": "^0.0.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-resizable": "^1.10.1",
    "react-simple-code-editor": "^0.11.0",
    "react-sortable-tree": "^2.8.0",
    "react-sortablejs": "^2.0.11",
    "react-stickynode": "^2.1.1",
    "react-stockcharts": "^0.7.8",
    "react-transition-group": "^4.3.0",
    "reactour": "^1.18.0",
    "reactstrap": "^8.5.1",
    "readline-sync": "^1.4.10",
    "sortablejs": "1.10.2",
    "store": "^2.0.12",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.4",
    "lint-staged": "^10.0.8",
    "prettier": "^2.2.1",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "umi": "^3.3.4",
    "yorkie": "^2.0.0"
  }
}